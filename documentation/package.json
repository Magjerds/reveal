{
  "name": "@cognite/reveal-documentation",
  "version": "0.0.1",
  "private": true,
  "author": "Maksim Nesterenko <maksim.nesterenko@cognite.com>",
  "contributors": [
    "Lars Moastuen <lars.moastuen@cognite.com>",
    "Joachim Bj√∏rne <joachim.bjorne@cognite.com>",
    "Christopher Tannum <christopher.tannum@cognite.com>",
    "Maksim Nesterenko <maksim.nesterenko@cognite.com>"
  ],
  "scripts": {
    "start": "yarn run reveal:copy && local-ssl-proxy --source 9000 --target 3000 | docusaurus start --no-open",
    "build": "yarn run reveal:copy && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "reveal:copy": "cpy ./node_modules/@cognite/reveal/*.wasm ./node_modules/@cognite/reveal/*.worker.js ./static --dot",
    "jsdoc2md": "jsdoc2md --files ../viewer/src/**/*.ts --configure ./jsdoc2md.json --partial src/jsdoc2md-partials/header.hbs > ./docs/api-reference_generated.md",
    "deploy": "docusaurus deploy"
  },
  "dependencies": {
    "@cognite/reveal": "^1.0.0-alpha-5",
    "@cognite/sdk": "link:../viewer/node_modules/@cognite/sdk",
    "@docusaurus/core": "2.0.0-alpha.58",
    "@docusaurus/module-type-aliases": "2.0.0-alpha.58",
    "@docusaurus/preset-classic": "2.0.0-alpha.58",
    "@docusaurus/theme-live-codeblock": "2.0.0-alpha.58",
    "clsx": "^1.1.1",
    "npm-to-yarn": "^1.0.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^5.1.1",
    "three": "link:../viewer/node_modules/three"
  },
  "devDependencies": {
    "@types/react": "^16.9.41",
    "@types/styled-components": "^5.1.1",
    "cpy-cli": "^3.1.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "local-ssl-proxy": "^1.3.0",
    "typescript": "^3.9.6"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
